.progress {
    width: 20px;
    height: 20px !important;
    float: left;
    line-height: 20px;
    background: none;
    box-shadow: none;
    position: relative;
    margin-right: 5px;

}

.progress:after {
    content: "";
    width: 100%;
    height: 100%;
    border-radius: 50%;

    background: #FF852D;
    position: absolute;
    top: 0;
    left: 0
}

.progress>span {
    width: 50%;
    height: 100%;
    overflow: hidden;
    position: absolute;
    top: 0;
    z-index: 1
}

.progress .progress-bar {
    width: 100%;
    height: 100%;
    background: none;
    position: absolute;
    background: #FCDA40;

    top: 0
}

.progress .progress-left .progress-bar {
    left: 100%;
    border-top-right-radius: 80px;
    border-bottom-right-radius: 80px;
    border-left: 0;
    -webkit-transform-origin: center left;
    transform-origin: center left
}

.progress .progress-right {
    right: 0
}

/* ============= Slider range  Input Value ======== */

.ui-slider .ui-slider-handle {
    width: 1em !important;
    height: 1em !important;
    cursor: default;
    border-radius: 50px !important;
    -moz-border-radius: 50px !important;
    -webkit-border-radius: 50px !important;
}

.ui-slider-handle .ui-state-default,
.ui-widget-content .ui-state-default {
    background: #0DF0FB !important;
    color: #0DF0FB !important;
    pointer-events: none !important;

}


.ui-slider-handle .ui-state-active,
.ui-widget-content .ui-state-active {

    background: #0DF0FB !important;
    background-image: none !important;
    border: 0 solid #EDC7D9 !important;
    font-weight: normal;
    color: #fff;
    border: 0px solid #0DF0FB !important;
}

.ui-slider-range.ui-corner-all.ui-widget-header.ui-slider-range-min {
    background: #FCDA40 !important;
}

.ui-widget-content {
    background-image: none !important;
    background: #FCDA40 !important;
    height: 7px !important;
    border: 1px solid #EDC7D9 !important;
    border-radius: 8px !important;
    -moz-border-radius: 3px !important;
    -webkit-border-radius: 3px !important;

}

/* ======END========= */
/* day1 */
.progress.day1 .progress-right .progress-bar {
    left: -100%;
    border-top-left-radius: 80px;
    border-bottom-left-radius: 80px;
    border-right: 0;
    -webkit-transform-origin: center right;
    transform-origin: center right;
    animation: loading-1 1.8s linear forwards
}

/* day2 */
.progress.day2 .progress-right .progress-bar {
    left: -100%;
    border-top-left-radius: 80px;
    border-bottom-left-radius: 80px;
    border-right: 0;
    -webkit-transform-origin: center right;
    transform-origin: center right;
    animation: loading-2 1.8s linear forwards
}

/* day3 */
.progress.day3 .progress-right .progress-bar {
    left: -100%;
    border-top-left-radius: 80px;
    border-bottom-left-radius: 80px;
    border-right: 0;
    -webkit-transform-origin: center right;
    transform-origin: center right;
    animation: loading-3 1.8s linear forwards
}


/* day4 */
.progress.day4 .progress-right .progress-bar {
    left: -100%;
    border-top-left-radius: 80px;
    border-bottom-left-radius: 80px;
    border-right: 0;
    -webkit-transform-origin: center right;
    transform-origin: center right;
    animation: loading-4 1.8s linear forwards
}


.progress.day4 .progress-left .progress-bar {
    animation: loading-5 1.5s linear forwards 1.8s
}

/* day5 */
.progress.day5 .progress-right .progress-bar {
    left: -100%;
    border-top-left-radius: 80px;
    border-bottom-left-radius: 80px;
    border-right: 0;
    -webkit-transform-origin: center right;
    transform-origin: center right;
    animation: loading-4 1.8s linear forwards
}



.progress.day5 .progress-left .progress-bar {
    animation: loading-6 1.5s linear forwards 1.8s
}

/* day6 */
.progress.day6 .progress-right .progress-bar {
    left: -100%;
    border-top-left-radius: 80px;
    border-bottom-left-radius: 80px;
    border-right: 0;
    -webkit-transform-origin: center right;
    transform-origin: center right;
    animation: loading-4 1.8s linear forwards
}



.progress.day6 .progress-left .progress-bar {
    animation: loading-7 1.5s linear forwards 1.8s
}

/* day7 */
.progress.day7 .progress-right .progress-bar {
    left: -100%;
    border-top-left-radius: 80px;
    border-bottom-left-radius: 80px;
    border-right: 0;
    -webkit-transform-origin: center right;
    transform-origin: center right;
    animation: loading-4 1.8s linear forwards
}



.progress.day7 .progress-left .progress-bar {
    animation: loading-8 1.5s linear forwards 1.8s
}


@keyframes loading-1 {
    0% {
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg)
    }

    100% {
        -webkit-transform: rotate(52deg);
        transform: rotate(52deg)
    }
}

@keyframes loading-2 {
    0% {
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg)
    }

    100% {
        -webkit-transform: rotate(104deg);
        transform: rotate(104deg)
    }
}

@keyframes loading-3 {
    0% {
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg)
    }

    100% {
        -webkit-transform: rotate(156deg);
        transform: rotate(156deg)
    }
}

@keyframes loading-4 {
    0% {
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg)
    }

    100% {
        -webkit-transform: rotate(180deg);
        transform: rotate(180deg)
    }
}

@keyframes loading-5 {
    0% {
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg)
    }

    100% {
        -webkit-transform: rotate(28deg);
        transform: rotate(28deg)
    }
}

@keyframes loading-6 {
    0% {
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg)
    }

    100% {
        -webkit-transform: rotate(80deg);
        transform: rotate(80deg)
    }
}

@keyframes loading-7 {
    0% {
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg)
    }

    100% {
        -webkit-transform: rotate(132deg);
        transform: rotate(132deg)
    }
}

@keyframes loading-8 {
    0% {
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg)
    }

    100% {
        -webkit-transform: rotate(180deg);
        transform: rotate(180deg)
    }
}

